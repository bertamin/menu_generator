<h1>New - Foods</h1>
<%= form_for @food do |f| %>
	<tr>
		<td><%= f.label :name  %></td>
		<td><%=  f.text_field :name %></td>
	</tr>
	<br>
	<tr>
		<td><%= f.label :existing_amount %></td>
		<td><%= f.text_field :existing_amount %></td>
	</tr>
	<br>
	<tr>
		<td>&nbsp;</td>
		<td id="button"><%= f.submit 'Save' %></td>
	</tr>
<% end %>